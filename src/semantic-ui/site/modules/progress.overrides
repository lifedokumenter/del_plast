/*******************************
         Site Overrides
*******************************/

@uiProgressBarRadius: 20px;
@uiProgressBarHeight: 1.444rem;

.ui.progress {
  background: @gray;
  border-radius: @uiProgressBarRadius;
  height: @uiProgressBarHeight;

  .bar {
    transition: width .3s;
    border-radius: @uiProgressBarRadius;
    height: @uiProgressBarHeight;
  }
}