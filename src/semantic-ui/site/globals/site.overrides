/*******************************
         Site Overrides
*******************************/


@font-face {
	font-family:'Italian Plate No2';
	src:	url('./fonts/ItalianPlateNo2-Extrabold.eot');
	src:	url('./fonts/ItalianPlateNo2-Extrabold.eot?#iefix') format('embedded-opentype'),
			url('./fonts/ItalianPlateNo2-Extrabold.woff2') format('woff2'),
			url('./fonts/ItalianPlateNo2-Extrabold.woff') format('woff');
	font-weight: 800;
	font-style: normal;
}

@font-face {
	font-family:'Italian Plate No2';
	src:	url('./fonts/ItalianPlateNo2-ExtraboldItalic.eot');
	src:	url('./fonts/ItalianPlateNo2-ExtraboldItalic.eot?#iefix') format('embedded-opentype'),
			url('./fonts/ItalianPlateNo2-ExtraboldItalic.woff2') format('woff2'),
			url('./fonts/ItalianPlateNo2-ExtraboldItalic.woff') format('woff');
	font-weight: 800;
	font-style: italic;
}

